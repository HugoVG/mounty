	</Binding>
	<Binding name="Water mount" category="Mounty">
		MountyKeyHandler("water")
	</Binding>
	<Binding name="Taxi mount" category="Mounty">
		MountyKeyHandler("taxi")
	</Binding>
	<Binding name="Repair mount" category="Mounty">
